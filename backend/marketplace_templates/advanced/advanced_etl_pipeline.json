  "updated_at": "2025-12-14T16:44:10.885142",
  "downloads": 0,
  "rating": 5.0,
  "template_type": "advanced",
  "multi_input_support": true,
  "multi_step_support": true,
  "multi_output_support": true,
  "pause_resume_support": true
}